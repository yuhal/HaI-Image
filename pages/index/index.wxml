<!--模版列表-->
<view class="mobileListBorder">
     <scroll-view scroll-y="true"  wx:if="{{items.length>0}}">
        <view class="main">
            <view wx:for="{{items}}" class="subMain">
                <view class="subMainImage">
                    <image src="../../image/background-down.png" class="downImage"/>
                    <image src="../../image/background-up.png" class="upImage"/>
                    <image class="images" data-src="{{items[index]}}" src="{{items[index]}}" bindtap="previewImage" />
                </view>
                <view class="subMainText">123</view>
            </view>
        </view>
    </scroll-view>
</view>